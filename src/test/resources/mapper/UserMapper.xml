<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kk.mybatis.binding.dao.IUserDao">
    <select id="queryUserName" parameterType="long" resultType="string">
        SELECT
             username
        FROM
              users
        WHERE
              id = #{id}
    </select>

    <select id="queryUserAge" resultType="java.lang.Integer" parameterType="java.lang.String">
        SELECT
             age
        FROM
              users
        WHERE
              id = #{id}
    </select>

</mapper>